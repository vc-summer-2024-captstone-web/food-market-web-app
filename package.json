{
  "name": "food-market",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.20.0 <19.0.0 || >=20.14.0"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:host": "astro dev --host",
    "dev:netlify": "netlify dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:remote": "astro check && astro build --remote",
    "build:netlify": "netlify build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "lint": "prettier --check . && astro check",
    "generate-example-env": "npx @dotenvx/dotenvx genexample",
    "cy:open": "cypress open",
    "test": "vitest",
    "test:vitest-coverage": "vitest --coverage",
    "test:vitest-ui": "vitest --ui",
    "test:serve": "cross-env TEST=true npm run dev",
    "test:watch": "vitest --watch",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.2",
    "@astrojs/db": "^0.11.7",
    "@astrojs/netlify": "^5.4.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@netlify/functions": "^2.8.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sendgrid/mail": "^8.1.3",
    "@sentry/astro": "^8.19.0",
    "astro": "^4.13.3",
    "handlebars": "^4.7.8",
    "lucia": "^3.2.0",
    "nodemailer": "^6.9.14",
    "ol": "^9.2.4",
    "react-icons": "^5.2.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.4"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.9.0",
    "@netlify/plugin-lighthouse": "^6.0.0",
    "@types/nodemailer": "^6.4.15",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "cross-env": "^7.0.3",
    "cypress": "^13.13.0",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "sass": "^1.77.8"
  }
}
